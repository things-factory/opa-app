    "@things-scene/excel": "*",
    "@things-scene/firebase": "*",
    "@things-scene/form": "*",
    "@things-scene/gauge": "*",
    "@things-scene/google-map": "*",
    "@things-scene/graphql": "*",
    "@things-scene/half-roundrect": "*",
